#include <iostream>
#include <vector>
#include <string>
#define ll long long
using namespace std;
int main(){
    string s;
  
    cin>>s;
    ll n=s.size();
     for(int i=0;i<n;i++){
        if(s[i]=='a'){
           s[i]='4';
        }else
        if(s[i]=='e'){
             s[i]='3';
         }else
         if(s[i]=='i'){
             s[i]='1';
         }else
         if(s[i]=='o'){
             s[i]='0';
         }else
         if(s[i]=='s'){
            s[i]='5';
         }
     }
 
 cout<<s;

}
